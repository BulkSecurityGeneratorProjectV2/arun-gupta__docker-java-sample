{  
   "id":"webapp",
   "apps":[  
      {  
         "id":"database",
         "cpus":4,
         "mem":4096,
         "instances":1,
         "container":{  
            "type":"DOCKER",
            "docker":{  
               "image":"arungupta/couchbase:travel",
               "network":"USER"
            }
         },
         "ipAddress":{  
            "networkName":"dcos"
         }
      },
      {  
         "id":"web",
         "dependencies":[
            "/webapp/database"
         ],
         "cpus":2,
         "mem":4096,
         "instances":1,
         "container":{  
            "type":"DOCKER",
            "docker":{  
               "image":"arungupta/wildfly-couchbase-javaee:travel",
               "network":"USER",
               "portMappings":[  
                  {  
                     "hostPort":0,
                     "containerPort":8080,
                     "protocol":"tcp"
                  }
               ]
            }
         },
         "ipAddress":{  
            "networkName":"dcos"
         },
         "env":{
            "COUCHBASE_URI":"database-webapp.marathon.containerip.dcos.thisdcos.directory"
         },
         "labels":{  
            "HAPROXY_0_VHOST":"DCOS-PublicSlaveLo-12LOS82TA7BGH-719211721.us-west-1.elb.amazonaws.com",
            "HAPROXY_GROUP":"external"
         }
      }
   ]
}
